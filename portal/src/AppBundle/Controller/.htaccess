<IfModule mod_rewrite.c>
    Options +FollowSymLinks
    RewriteEngine On
    RewriteBase /

    # Explicitly disable rewriting for front controllers
    RewriteRule ^/melnyk/test/new/web/app_dev.php - [L]
    RewriteRule ^/melnyk/test/new/web/Ã­ndex.php - [L]
    RewriteRule ^/melnyk/test/new/web/app.php - [L]

    # Fix the bundles folder
    RewriteRule ^bundles/(.*)$ /melnyk/test/new/web/bundles/$1  [QSA,L]

    RewriteCond %{REQUEST_FILENAME} !-f
    # Change below before deploying to production
    #RewriteRule ^(.*)$ /melnyk/web/app.php [QSA,L]
